            0.6 -0.012
            0.8 -0.005
            0.95 -0.001
            1.0 0.0
          </pointList>
        </airfoil>
        <airfoil uID="af_tip">
          <name>NACA2412-tip</name>
          <pointList>
            1.0 0.0
            0.95 0.012
            0.8 0.028
            0.6 0.036
            0.4 0.030
            0.2 0.018
            0.0 0.0
            0.2 -0.012
