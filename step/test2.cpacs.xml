  <vehicles>
    <profiles>
      <airfoils>
        <airfoil uID="af_root">
          <name>NACA2412-root</name>
          <pointList>
            1.0 0.0
            0.95 0.015
            0.8 0.035
            0.6 0.045
            0.4 0.038
            0.2 0.025
            0.0 0.0
            0.2 -0.015
