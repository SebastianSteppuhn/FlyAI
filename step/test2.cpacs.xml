            1.0 0.0
          </pointList>
        </airfoil>
      </airfoils>
    </profiles>
    <aircraft>
      <model uID="trainer1">
        <name>Single-engine Two-seat Trainer</name>
        <wings>
          <wing uID="wing1">
            <name>Main Wing</name>
            <symmetry>y</symmetry>
            <sections>
              <section uID="sec_root">
                <name>Root Section</name>
                <elements>
                  <element uID="elem_root">
                    <airfoilUID>af_root</airfoilUID>
                    <transformation>
                      <translation>
                        0.0 0.0 0.0
                      </translation>
                      <rotation>
                        0.0 0.0 0.0
                      </rotation>
                      <scaling>
                        1.0 1.0 1.0
                      </scaling>
                    </transformation>
                    <width>0.0</width>
                  </element>
                </elements>
              </section>
              <section uID="sec_tip">
                <name>Tip Section</name>
                <elements>
                  <element uID="elem_tip">
                    <airfoilUID>af_tip</airfoilUID>
                    <transformation>
                      <translation>
                        5.0 0.0 0.0
                      </translation>
                      <rotation>
                        0.0 0.0 0.0
                      </rotation>
                      <scaling>
                        1.0 1.0 1.0
                      </scaling>
                    </transformation>
                    <width>0.0</width>
                  </element>
                </elements>
              </section>
            </sections>
            <positionings>
              <positioning uID="pos_root_tip">
                <fromSection>sec_root</fromSection>
                <toSection>sec_tip</toSection>
                <length>5.0</length>
                <sweepAngle>2.0</sweepAngle>
                <dihedralAngle>3.0</dihedralAngle>
                <x>0.0</x>
                <y>0.0</y>
                <z>0.0</z>
              </positioning>
            </positionings>
            <segments>
              <segment uID="seg1">
                <name>Root to Tip</name>
                <startSectionUID>sec_root</startSectionUID>
                <startElementUID>elem_root</startElementUID>
                <endSectionUID>sec_tip</endSectionUID>
                <endElementUID>elem_tip</endElementUID>
              </segment>
            </segments>
          </wing>
        </wings>
      </model>
    </aircraft>
  </vehicles>
</cpacs>